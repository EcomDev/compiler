{
    "name": "ecomdev/compiler",
    "description": "Compiler of parseable data structures from (XML, JSON, etc) into PHP code that can be included into application",
    "type": "library",
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "^2.3",
        "satooshi/php-coveralls": "dev-master",
        "ciaranmcnulty/phpspec-typehintedmethods": "^1.1",
        "mikey179/vfsStream": "^1.6",
        "php": ">= 5.4",
        "henrikbjorn/phpspec-code-coverage": "^2.0",
        "phpspec/nyan-formatters": "^1.0",
        "ecomdev/phpspec-file-matcher": "^1.0"
    },
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Ivan Chepurnyi",
            "email": "ivan@ecomdev.org"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "EcomDev\\Compiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\EcomDev\\Compiler\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
